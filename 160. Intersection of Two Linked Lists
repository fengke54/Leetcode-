# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def getIntersectionNode(self, headA: ListNode, headB: ListNode) -> ListNode:
        nodesB = set()
        
        while headB is not None:
            nodesB.add(headB)
            headB = headB.next
        
        while headA is not None:
            if headA in nodesB:
                return headA
            headA = headA.next # find the nodes that reside in both A and B, intersection
            
        return None 
#         while headA is not None:
#             pB = headB
#         while pB is not None:
#             if headA == pB:
#                 return headA # Brute Force solution checks if A is in list B until they reach the intersection 
#             pB = pB.next
#             headA = headA.next
        
#         return None
# Brute force is not working because time limit exeed
